.animation{
  -webkit-animation: rotate 2s linear;
  animation: rotate 2s linear;
}
.biubiu{
  -webkit-transform:rotate(720deg) scale(2,2);
  transform:rotate(720deg) scale(0,0); 
  -webkit-transition:all 2s ease-in-out;
  transition:all 2s ease-in-out;
}


.bg-bubbles {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.bg-bubbles li {
  position: absolute;
  bottom: -160rpx;
  width: 40rpx;
  height: 40rpx;
  /* background-color: rgba(255,255,255,0.15); */
  list-style: none;
  animation: square 15s infinite;
  transition-timing-function: linear;
  overflow: hidden;
}
li image{
  width: 100%;
  height: 100%;
}
.bg-bubbles li:nth-child(1) {
  left: 10%;
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.bg-bubbles li:nth-child(2) {
  left: 20%;
  width: 90rpx;
  height: 90rpx;
  animation-delay: 2s;
  animation-duration: 7s;
  background-image: url(data:image/svg+xml;base64,);
    background-size: cover;
}
.bg-bubbles li:nth-child(3) {
  left: 25%;
  animation-delay: 4s;
 
}
.bg-bubbles li:nth-child(4) {
  left: 40%;
  width: 120rpx;
  height: 120rpx;
  animation-duration: 8s;
  /* background-color: rgba(255,255,255,0.3); */
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.bg-bubbles li:nth-child(5) {
  left: 70%;
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.bg-bubbles li:nth-child(6) {
  left: 80%;
  width: 120rpx;
  height: 120rpx;
  animation-delay: 3s;
  /* background-color: rgba(255,255,255,0.2); */
  background-image: url(data:image/svg+xml;base64,);
  background-size: contain;
}
.bg-bubbles li:nth-child(7) {
  left: 32%;
  width: 160rpx;
  height: 160rpx;
  animation-delay: 2s;
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.bg-bubbles li:nth-child(8) {
  left: 55%;
  width: 50rpx;
  height: 50rpx;
  animation-delay: 4s;
  animation-duration: 15s;
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
.bg-bubbles li:nth-child(9) {
  left: 25%;
  width: 80rpx;
  height: 80rpx;
  animation-delay: 2s;
  animation-duration: 12s;
  /* background-color: rgba(255,255,255,0.3); */
  background-image:url(data:image/svg+xml;base64,);
  background-size: cover;
}
.bg-bubbles li:nth-child(10) {
  left: 80%;
  width: 160rpx;
  height: 160rpx;
  animation-delay: 5s;
  background-image: url(data:image/svg+xml;base64,);
  background-size: cover;
}
@keyframes square {
  0% {
    opacity: 0.5;
    transform: translateY(0rpx) rotate(45deg);
  }
  25% {
    opacity: 0.75;
    transform: translateY(-800rpx) rotate(90deg);
  }
  50% {
    opacity: 1;
    transform: translateY(-1200rpx) rotate(135deg);
  }
  100% {
    opacity: 0;
    transform: translateY(-1000rpx) rotate(180deg);
  }
}

@keyframes rotate{
  0%{
    -webkit-transform: rotate(0);
    transform: rotate(0);
  }
  50%{
    -webkit-transform: rotate(-180deg);
    transform: rotate(-180deg);
  }
  100%{
    -webkit-transform: rotate(-360deg);
    transform: rotate(-360deg);
  }
}