.flex-wrap{
    display: flex;
    height: 100%;
    flex-direction: column;
    justify-content: space-around;
    align-items: center;
}
.flex-item{
    /* 相对于750px */
        width: 700rpx;
        height: 200rpx;
        line-height: 200rpx;
        text-align: center;
}
.bc_green{
    background-color: green;
}
.bc_red{
    background-color: red;
    align-self: flex-end;
}
.bc_blue{
    background-color: blue;
}